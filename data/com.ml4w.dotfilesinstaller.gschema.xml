<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="dotfiles-installer">
	<schema id="com.ml4w.dotfilesinstaller" path="/com/ml4w/dotfilesinstaller/">
  	  <key name="my-dotfiles-folder" type="s">
	    <default>'.mydotfiles'</default>
	  </key>
  	  <key name="my-enable-symlinks" type="b">
	    <default>true</default>
	  </key>
  	  <key name="my-enable-dev" type="b">
	    <default>false</default>
	  </key>
	  <key name="my-dev-sync-confirm" type="b">
	    <default>true</default>
	  </key>
	</schema>
</schemalist>
